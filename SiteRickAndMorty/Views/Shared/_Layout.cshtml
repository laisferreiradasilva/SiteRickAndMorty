<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Site Rick and Morty</title>
    <style>
        .div_content {
            width: 800px;
            margin-left: auto;
            margin-right: auto;
        }

        a {
            text-decoration: none
        }
    </style>
    <style>
        table {
            font-family: arial, sans-serif;
            border-collapse: collapse;
            width: 100%;
        }

        td, th {
            border: 1px solid #dddddd;
            text-align: left;
            padding: 8px;
        }

        tr:nth-child(even) {
            background-color: #dddddd;
        }
    </style>
    <script type="text/javascript">
        function countFunction() {
            var x = document.getElementById("SearchName").value;
            document.getElementById("label_count").innerHTML = "Total character: " + x.length;
        }
    </script>
</head>
<body>
    <div class="div_content">
        @RenderBody()
    </div>
</body>
</html>
